{
  "manifest_version": 2,
  "name": "Seeker",
  "description": "List the data of a website for citetation and bibliography",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "js": ["popup.js", "getMetaData.js"]
    }
  ]
}